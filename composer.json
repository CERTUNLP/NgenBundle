{
  "name": "certunlp/ngen-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides a incident management for certs",
  "keywords": [
    "cert",
    "unlp",
    "incident",
    "management",
    "report",
    "system"
  ],
  "homepage": "https://github.com/CERTUNLP/NgenBundle",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "CERT UNLP",
      "email": "support@cert.unlp.edu.ar"
    }
  ],
  "support": {
    "issues": "https://github.com/CERTUNLP/NgenBundle/issues"
  },
  "autoload": {
    "psr-4": {
      "CertUnlp\\NgenBundle\\": ""
    }
  },
  "require": {
    "php": ">=7.2",
    "symfony/symfony": "2.8.*",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "twig/extensions": "~1.2",
    "gedmo/doctrine-extensions": "~2.4",
    "components/jquery": "~2.1",
    "friendsofsymfony/elastica-bundle": "4.*",
    "friendsofsymfony/comment-bundle": "^2.2",
    "friendsofsymfony/user-bundle": "=2.0.1",
    "ninsuo/symfony-collection": "2.1.30",
    "knplabs/knp-markdown-bundle": "~1.4",
    "knplabs/knp-menu-bundle": "~2.0",
    "knplabs/knp-paginator-bundle": "^2.8",
    "twbs/bootstrap": "~4.3.1",
    "fortawesome/font-awesome":"~5.9.0",
    "kbsali/redmine-api": "~1.5",
    "nelmio/api-doc-bundle": "2.13.3",
    "fabpot/goutte": "~2.0",
    "ddeboer/data-import": "~0.18.0",
    "ddeboer/data-import-bundle": "~0.1.0",
    "stfalcon/tinymce-bundle": "~0.3.8",
    "doctrine/doctrine-migrations-bundle": "^1.1",
    "symfony/assetic-bundle": "^2.7",
    "symfony/security-acl": "^3.0",
    "beberlei/DoctrineExtensions": "^1.1",
    "darsyn/ip": "^4.0",
    "tetranz/select2entity-bundle": "^2.9",
    "ext-json": "*",
    "ext-curl": "*",
    "secit-pl/imap-bundle":"*"
  },
  "require-dev": {
    "symfony/assetic-bundle": "^2.7",
    "symfony/browser-kit": "~2.3",
    "symfony/css-selector": "~2.3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall"
    ],
    "post-update-cmd": [
      "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate"
    ]
  }
}
