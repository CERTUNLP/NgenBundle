{
  "name": "certunlp/ngen-bundle",
  "license": "GPL-3.0",
  "type": "symfony-bundle",
  "description": "This Bundle provides a incident management for certs",
  "keywords": [
    "cert",
    "unlp",
    "incident",
    "management",
    "report",
    "system"
  ],
  "homepage": "https://github.com/CERTUNLP/NgenBundle",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "CERT UNLP",
      "email": "support@cert.unlp.edu.ar"
    }
  ],
  "support": {
    "issues": "https://github.com/CERTUNLP/NgenBundle/issues"
  },
  "autoload": {
    "psr-4": {
      "CertUnlp\\NgenBundle\\": "src/NgenBundle/",
      "AppBundle\\": "src/AppBundle"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
    ]
  },
  "require": {
    "php": ">=7.2",
    "cmen/google-charts-bundle": "^3.2",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-migrations-bundle": "^2.1",
    "doctrine/orm": "^2.5",
    "friendsofsymfony/comment-bundle": "^3.0",
    "friendsofsymfony/elastica-bundle": "^5.1",
    "friendsofsymfony/user-bundle": "^2.1",
    "gedmo/doctrine-extensions": "^2.4",
    "incenteev/composer-parameter-handler": "^2.0",
    "knplabs/knp-paginator-bundle": "^4.1",
    "knplabs/knp-components": "v2.3.3",
    "certunlp/rdap-client": "~1.1",
    "nelmio/api-doc-bundle": "^3.6",
    "ninsuo/symfony-collection": "^2.1",
    "rollerworks/password-strength-bundle": "^2.2",
    "secit-pl/imap-bundle": "^1.3",
    "sensio/distribution-bundle": "^5.0.19",
    "sensio/framework-extra-bundle": "^5.0.0",
    "stfalcon/tinymce-bundle": "^2.4",
    "stof/doctrine-extensions-bundle": "v1.3",
    "symfony/assetic-bundle": "^2.8",
    "symfony/monolog-bundle": "^3.1.0",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/swiftmailer-bundle": "^2.6.4",
    "symfony/symfony": "3.4.*",
    "tetranz/select2entity-bundle": "^2.10",
    "twig/twig": "^1.0||^2.0",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "twig/extensions": "~1.5",
    "components/jquery": "~3.4",
    "twbs/bootstrap": "~4.4.1",
    "fortawesome/font-awesome": "~5.13.0",
    "beberlei/doctrineextensions": "~1.2",
    "darsyn/ip": "^4.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "symfony/phpunit-bridge": "^3.0"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall"
    ],
    "post-update-cmd": [
      "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": null
  }
}
