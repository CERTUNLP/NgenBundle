{% import '@CertUnlpNgen/Frontend/detailMacros.html.twig' as print %}
<tr data-id="{{ networkEntity.slug }}">
    <td class="text-center">
        {% include '@CertUnlpNgen/Frontend/list/listDropdown.html.twig' with {'object':networkEntity , 'detail_link': path('cert_unlp_ngen_network_entity_detail_id',{ 'id': networkEntity.id}),'edit_link': path('cert_unlp_ngen_network_entity_edit_id',{'id': networkEntity.id})} %}
    </td>
    <td id="state_label_holder" class="text-center">
        {{ print.statusdropbox(networkEntity) }}
    </td>
    <td class="text-center">
        <div class="float-left">
            {{ print.colordropbox(networkEntity.color,networkEntity.icon,networkEntity.name,'#',networkEntity.name) }}
        </div>
    </td>
    <td class="text-center">
        <small>
            {{ print.colordropbox('primary','calendar-alt',networkEntity.createdAt|date('Y-m-d'),'#',networkEntity.createdAt|date('Y-m-d')) }}
        </small>
    </td>
    <td class="text-center">
        <small>
            {{ print.colordropbox('primary','calendar-alt',networkEntity.updatedAt|date('Y-m-d'),'#',networkEntity.updatedAt|date('Y-m-d')) }}
        </small>
    </td>
</tr>