{% extends 'CertUnlpNgenBundle::layout.html.twig' %}
{% block title %}Incidents{% endblock %}
{% block content %}
    <div class="row">
        <div class="col-lg-12">
            {% block content_header %}
                <h1 class="page-header">Incidents</h1>
            {% endblock %}
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            {% block content_body %}
            {% endblock %}
        </div>
    </div>


{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/certunlpngen/css/ladda-themeless.min.css') }}" media="screen" />
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    {% javascripts
    '@CertUnlpNgenBundle/Resources/public/js/api/*'
    '@CertUnlpNgenBundle/Resources/public/js/*'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
