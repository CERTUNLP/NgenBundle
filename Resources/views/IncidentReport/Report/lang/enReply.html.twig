{% extends  'CertUnlpNgenBundle:IncidentReport:Report/base/baseReport.html.twig' %}
{% block greeting %}
    Estimated {{ incident.networkAdmin }},
{% endblock %}
{% block report_content %}
    <tr>
        <td>
            {% if(body) %}
                {{ body|striptags('<div>,<a>,<pre>,<code>')|raw }}
            {% endif %}
        </td>
        <td class="expander"></td>
    </tr>
{% endblock %}
{% block footer_content %}
    <div class="destacated">
        PLEASE DO NOT REPLY TO THIS MAIL.
    </div>

    We would appreciate that you notify us when you solve the problem.
    For any questions, please contact us to <strong>abuse@cert.unlp.edu.ar</strong>.

    Regards, CertUnlp Support.

    CERTunlp - http://www.cert.unlp.edu.ar

{% endblock %}
{% block footer_generated_by %}

    This report has been generated with <a href="https://github.com/CERTUNLP/NgenBundle">https://github.com/CERTUNLP/NgenBundle</a>

{% endblock %}