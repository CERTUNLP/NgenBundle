<tr data-id="{{ network.addressAndMask }}">
    <td class="align-middle">
        {{ network.addressAndMask }}
    </td>
    <td class="align-middle">
        {{ network.type |trans }}
    </td>
    <td class="align-middle">
        {{ network.networkEntity }}
    </td>
    <td class="align-middle">
        {{ network.networkAdmin }}
    </td>
    <td id="state_label_holder" class="align-middle">
        <div>
            {% include "CertUnlpNgenBundle:Network:Frontend/list/stateLabel.html.twig" with {'network': network} %}
        </div>
    </td>
    <td class="align-middle">
        {% set detail_link %}
            {% include '@CertUnlpNgen/Network/Frontend/list/networkLink.html.twig' with {'action':'detail'} %}
        {% endset %}
        {% set edit_link %}
            {% include '@CertUnlpNgen/Network/Frontend/list/networkLink.html.twig' with {'action':'edit'} %}
        {% endset %}
        {% include '@CertUnlpNgen/Frontend/list/listDropdown.html.twig' with {'object':network , 'detail_link':detail_link ,'edit_link': edit_link} %}
</tr>